{
  "name": "@smite/browser-automation",
  "version": "2.0.0",
  "description": "Browser automation plugin for SMITE using MCP servers - Web search, content reading, image analysis, and GitHub research",
  "author": {
    "name": "Pamacea",
    "email": "contact@pamacea.com",
    "url": "https://github.com/pamacea"
  },
  "license": "MIT",
  "homepage": "https://github.com/pamacea/smite#browser-automation",
  "repository": {
    "type": "git",
    "url": "https://github.com/pamacea/smite.git",
    "directory": "plugins/browser-automation"
  },
  "bugs": {
    "url": "https://github.com/pamacea/smite/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "smite-browse": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "demo:performance": "tsc && node dist/examples/performance-demo.js"
  },
  "smite": {
    "type": "tool",
    "minVersion": "1.0.0",
    "configFile": ".claude/.smite/browser.json",
    "autoInstall": true
  },
  "keywords": [
    "smite",
    "smite-plugin",
    "browser",
    "automation",
    "mcp",
    "web-search",
    "web-scraping",
    "image-analysis",
    "github",
    "research"
  ],
  "dependencies": {
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "typescript": "^5.7.3",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.39.2"
  }
}
